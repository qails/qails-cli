{
  "name": "qails-cli",
  "version": "1.1.1",
  "license": "MIT",
  "description": "The Qails CLI for project setup",
  "repository": "qails/qails-cli",
  "bin": {
    "qails": "./dist/bin/qails.js"
  },
  "files": [
    "dist",
    "templates"
  ],
  "scripts": {
    "build": "rimraf dist && babel src -d dist",
    "start": "babel-node src/bin/qails.js",
    "dist": "node dist/bin/qails.js",
    "lint": "eslint --quiet \"**/*.js\"",
    "release": "standard-version && npm publish",
    "prepublish": "npm run build",
    "postrelease": "git push && git push origin --tags"
  },
  "dependencies": {
    "babel-cli": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-preset-env": "^1.3.3",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.24.1",
    "commander": "^2.9.0",
    "eslint": "^3.19.0",
    "eslint-config-qunar": "^1.1.4",
    "eslint-plugin-import": "^2.2.0",
    "inquirer": "^3.0.6",
    "mem-fs": "^1.1.3",
    "mem-fs-editor": "^3.0.2",
    "packing-glob": "^1.0.1",
    "pluralize": "^5.0.0",
    "rimraf": "^2.6.2"
  },
  "engines": {
    "node": ">=4",
    "npm": ">=3"
  }
}
