{
  "name": "qails-cli",
  "version": "1.1.11",
  "license": "MIT",
  "description": "一个快速生成 qails 网站的手脚架工具",
  "repository": "qails/qails-cli",
  "bin": {
    "qails": "dist/bin/qails.js"
  },
  "files": [
    "dist",
    "templates"
  ],
  "scripts": {
    "build": "rimraf dist && babel src -d dist",
    "start": "babel-node src/bin/qails.js",
    "dist": "node dist/bin/qails.js",
    "lint": "eslint --quiet \"**/*.js\"",
    "release": "standard-version && npm publish",
    "prepublish": "npm run build",
    "postrelease": "git push && git push origin --tags"
  },
  "dependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.0.2",
    "babel-preset-env": "^1.6.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.26.0",
    "command-exists": "^1.2.2",
    "commander": "^2.11.0",
    "eslint": "^4.11.0",
    "eslint-config-qunar": "^1.2.1",
    "eslint-plugin-import": "^2.8.0",
    "inquirer": "^4.0.0",
    "mem-fs": "^1.1.3",
    "mem-fs-editor": "^3.0.2",
    "packing-glob": "^1.0.1",
    "pluralize": "^7.0.0",
    "rimraf": "^2.6.2"
  },
  "engines": {
    "node": ">=4",
    "npm": ">=3"
  }
}
