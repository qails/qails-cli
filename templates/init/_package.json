{
  "name": "<%=appname%>",
  "version": "0.0.0",
  "main": "src/index.js",
  "scripts": {
    "build": "babel src -d dist",
    "init": "yarn migrate:rollback && yarn migrate:latest && yarn seed:run",
    "lint": "eslint --quiet \"**/*.js\"",
    "migrate:rollback": "babel-node ./node_modules/.bin/knex --knexfile=src/config/knexfile.js migrate:rollback",
    "migrate:latest": "babel-node ./node_modules/.bin/knex --knexfile=src/config/knexfile.js migrate:latest",
    "seed:run": "babel-node ./node_modules/.bin/knex --knexfile=src/config/knexfile.js seed:run",
    "release": "standard-version",
    "postinstall": "babel-node scripts/postinstall.js",
    "postrelease": "git push && git push origin --tags",
    "start": "pm2 start ecosystem.config.js",
    "test": "mocha --recursive --compilers js:babel-core/register && yarn lint"
  },
  "dependencies": {
    "axios": "^0.16.1",
    "babel-cli": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-preset-env": "^1.3.3",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.24.1",
    "bookshelf": "^0.10.3",
    "bookshelf-cascade-delete": "^2.0.1",
    "bookshelf-json-columns": "zhongzhi107/bookshelf-json-columns",
    "bookshelf-mask": "^2.0.1",
    "bookshelf-modelbase": "^2.10.3",
    "bookshelf-uuid": "^1.0.0",
    "dotenv": "^4.0.0",
    "eslint": "^3.19.0",
    "eslint-config-qunar": "^1.1.4",
    "eslint-plugin-import": "^2.2.0",
    "file-stream-rotator": "0.1.0",
    "joi": "^10.4.2",
    "knex": "^0.13.0",
    "koa": "^2.2.0",
    "koa-bodyparser": "^4.2.0",
    "koa-compose": "^4.0.0",
    "koa-convert": "^1.2.0",
    "koa-json": "^2.0.2",
    "koa-morgan": "^1.0.1",
    "koa-qs": "^2.0.0",
    "koa-router": "^7.1.1",
    "koa-session": "^5.0.0",
    "lodash": "^4.17.4",
    "minimatch": "^3.0.4",
    "mkdirp": "^0.5.1",
    "mysql": "^2.13.0",
    "object-assign": "^4.1.1",
    "packing-glob": "^1.0.1",
    "require-all": "^2.2.0",
    "statsd-client": "^0.2.4",
    "uuid": "^3.0.1"
  },
  "devDependencies": {
    "casual": "^1.5.12",
    "mocha": "^3.4.1",
    "pre-commit": "^1.2.2"
  },
  "peerDependencies": {
    "pm2": "2.x"
  },
  "engines": {
    "npm": ">= 3.0",
    "node": ">= 7.6"
  },
  "pre-commit": [
    "test"
  ]
}
